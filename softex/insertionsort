import random

def insertionsort (array):
    for step in range(1, len(array)):
        key = array[step]
        j = step -1
        
        
        while j >= 0 and key < array[j]:
            array [j + 1] = array[j]
            j = j - 1
            
        array[j + 1] = key
        
data = [29, 1, 4, 5, 7, 2, 30, 21, 25, 17, 27, 13, 14, 15, 14, 16, 18, 24, 26, 6, 22, 9, 23, 12, 11, 8, 28, 3, 19, 26]
insertionsort(data)
print ('Array em ordem crescente')
print (data)
